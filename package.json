  "author": "ramonqu",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/vertex-sdk": "^0.14.2",
    "@google-cloud/vertexai": "^1.10.0",
    "@google/genai": "^1.0.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "express": "^4.18.2",
    "js-yaml": "^4.1.1",
    "ora": "^9.1.0",
    "prom-client": "^15.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "engines": {
